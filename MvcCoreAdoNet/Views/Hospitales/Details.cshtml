@model Hospital 

<div class="container">
	<h1>Detalles</h1>
	<ul class="list-group">
		<li class="list-group-item">
			Id: @Model.IdHospital
		</li>
		<li class="list-group-item">
			Nombre: @Model.Nombre
		</li>
		<li class="list-group-item">
			Telefono: @Model.Telefono
		</li>
		<li class="list-group-item">
			Camas: @Model.Camas
		</li>
	</ul>
	<button class="btn btn-warning">
		@Html.ActionLink("Volver", "Index", "Hospitales")
	</button>
</div>
